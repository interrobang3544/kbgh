        <iframe id="zep" src="https://zep.us/play/24G0RJ" allow-popups: 0; width="100%" height="350px"></iframe>
      <% } else { %>
        <div class="app-box">
          <div id="zep-app">
            <a href="/zep"><div class="link"><img class="main-app-icon" src="/images/zep.png"></div></a>
          </div>
